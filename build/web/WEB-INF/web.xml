<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ClientSignUp</servlet-name>
        <servlet-class>package1.serviceProvider</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>serviceProvider</servlet-name>
        <servlet-class>package1.serviceProvider</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>admin</servlet-name>
        <servlet-class>classes.admin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>spPost</servlet-name>
        <servlet-class>package1.spPost</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>spLog</servlet-name>
        <servlet-class>package1.spLog</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>createUname</servlet-name>
        <servlet-class>package1.createUname</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>proPic</servlet-name>
        <servlet-class>package1.proPic</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>cusSignUp</servlet-name>
        <servlet-class>package1.cusSignUp</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ClientSignUp</servlet-name>
        <url-pattern>/ClientSignUp</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>serviceProvider</servlet-name>
        <url-pattern>/serviceProvider</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>admin</servlet-name>
        <url-pattern>/admin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>spPost</servlet-name>
        <url-pattern>/spPost</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>spLog</servlet-name>
        <url-pattern>/spLog</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>createUname</servlet-name>
        <url-pattern>/createUname</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>proPic</servlet-name>
        <url-pattern>/proPic</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>cusSignUp</servlet-name>
        <url-pattern>/cusSignUp</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
